{
  "manifest_version": 3,
  "name": "Plex Skipper",
  "version": "1.1.3",
  "description": "Automatically clicks the 'Skip Intro' and 'Skip Credits' buttons on Plex.",
  "content_scripts": [
    {
      "matches": ["*://app.plex.tv/*"],
      "js": ["skipper.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  }
}